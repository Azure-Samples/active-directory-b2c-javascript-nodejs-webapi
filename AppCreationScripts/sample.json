{
    "Sample": {
      "Title": "A sample demonstrating how to protect a Node.js web API with Azure AD B2C using the Passport.js library",
      "Level": 200,
      "Client": "JavaScript SPA",
      "Service": "Node.js Web Api",
      "RepositoryUrl": "active-directory-b2c-javascript-nodejs-webapi",
      "Endpoint": "AAD v2.0",
      "Provider": "B2C"
    },
    "AADApps": [
      {
        "Id": "service",
        "Name": "active-directory-b2c-javascript-nodejs-webapi",
        "Kind": "WebApi",
        "HomePage": "http://localhost:5000/",
        "ReplyUrls": "http://localhost:5000/",
        "Audience": "AzureADAnyOrgandPersonalMicrosoftAccount"
      }
    ],
    "CodeConfiguration": [
      {
        "App": "service",
        "SettingKind": "JSON",
        "SettingFile": "\\..\\auth.json",
        "Mappings": [
          {
            "key": "clientID",
            "value": ".AppId"
          },
          {
            "key": "tenantID",
            "value": "$tenantId"
          },
          {
            "key": "audience",
            "value": ".AppId"
          }
        ]
      }
    ]
  }